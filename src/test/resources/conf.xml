<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test suite" parallel="classes" thread-count="10">
    <test name="Default tests" verbose="1" thread-count="10" parallel="classes">
        <parameter name="driver" value="com.mysql.jdbc.Driver"/>
        <parameter name="url" value="jdbc:mysql://localhost:3306/db"/>

        <groups>
            <run>
                <exclude name="integration"/>
            </run>
        </groups>

        <packages>
            <package name="com.jaspersoft.jasperserver.jaxrs.client.*"/>
        </packages>
    </test>
</suite>