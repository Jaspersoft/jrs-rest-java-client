        <Measure name="Store Cost" column="store_cost" aggregator="sum"
                 formatString="#,###.00"/>
        <Measure name="Store Sales" column="store_sales" aggregator="sum"
                 formatString="#,###.00"/>
        <Measure name="Sales Count" column="product_id" aggregator="count"
